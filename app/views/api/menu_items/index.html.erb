<p id="notice"><%= notice %></p>

<h1>Api Menu Items</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_menu_items.each do |api_menu_item| %>
      <tr>
        <td><%= link_to 'Show', api_menu_item %></td>
        <td><%= link_to 'Edit', edit_api_menu_item_path(api_menu_item) %></td>
        <td><%= link_to 'Destroy', api_menu_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api Menu Item', new_api_menu_item_path %>
