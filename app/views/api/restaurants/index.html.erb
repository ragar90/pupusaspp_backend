<p id="notice"><%= notice %></p>

<h1>Api Restaurants</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_restaurants.each do |api_restaurant| %>
      <tr>
        <td><%= link_to 'Show', api_restaurant %></td>
        <td><%= link_to 'Edit', edit_api_restaurant_path(api_restaurant) %></td>
        <td><%= link_to 'Destroy', api_restaurant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api Restaurant', new_api_restaurant_path %>
