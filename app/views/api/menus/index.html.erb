<p id="notice"><%= notice %></p>

<h1>Api Menus</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_menus.each do |api_menu| %>
      <tr>
        <td><%= link_to 'Show', api_menu %></td>
        <td><%= link_to 'Edit', edit_api_menu_path(api_menu) %></td>
        <td><%= link_to 'Destroy', api_menu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api Menu', new_api_menu_path %>
